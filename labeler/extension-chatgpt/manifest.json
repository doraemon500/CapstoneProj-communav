{
  "manifest_version": 3,
  "name": "ChatGPT Browser Labeler",
  "version": "1.0",
  "permissions": [],
  "host_permissions": [
    "http://retn0.iptime.org:11050/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://chatgpt.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
